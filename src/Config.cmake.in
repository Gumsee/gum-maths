@PACKAGE_INIT@

#include("@CMAKE_INSTALL_FULL_LIBDIR@/cmake/@PROJECT_NAME@/@PROJECT_NAME@Targets.cmake")

check_required_components(@PROJECT_NAME@)

set(GUMMATHS_INCLUDE_DIRS   "@CMAKE_INSTALL_FULL_INCLUDEDIR@/@PROJECT_NAME@" )
find_library(GUMMATHS_LIBRARIES NAMES @PROJECT_NAME@
    PATHS
    "/usr/lib"
    "/usr/local/lib"
    "@CMAKE_INSTALL_FULL_LIBDIR@"
)

set(GUMMATHS_FOUND TRUE)
if(NOT GUMMATHS_LIBRARIES)
    set(GUMMATHS_FOUND FALSE)
endif()
