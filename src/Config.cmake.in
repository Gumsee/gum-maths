@PACKAGE_INIT@

include("@CMAKE_INSTALL_FULL_LIBDIR@/cmake/@CMAKE_PROJECT_NAME@/@CMAKE_PROJECT_NAME@Targets.cmake")

check_required_components(@CMAKE_PROJECT_NAME@)

set(GUMMATHS_INCLUDE_DIRS   "@CMAKE_INSTALL_FULL_INCLUDEDIR@/@CMAKE_PROJECT_NAME@" )
find_library(GUMMATHS_LIBRARIES NAMES @CMAKE_PROJECT_NAME@
    PATHS
    /usr/lib
    /usr/local/lib
)

set(GUMMATHS_FOUND TRUE)
if(NOT GUMMATHS_LIBRARIES)
    set(GUMMATHS_FOUND FALSE)
endif()
